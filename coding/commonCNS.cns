; Krillin's Common state file
;---------------------------------------------------------------------------
; Stand
[Statedef 0]
type = S
physics = S
sprpriority = 0

[State 0, Anim]
type = ChangeAnim
trigger1 = pos y < 0
value = 41+var(0)

[State 0, ctrlset]
type = CtrlSet
trigger1 = anim = 41+var(0)
value = 0

[State 0, Anim]
type = ChangeAnim
triggerall = pos y >= 0
trigger1 = Anim != 0+var(0) && Anim != 5+var(0)
trigger2 = anim = 5+var(0) && AnimTime = 0 ;Turn anim over+var(0)
value = 0+var(0)

[State 0, Vel]
type = VelSet
trigger1 = Time = 0
y = 0

[State 0, Vel] ;Stop moving if low velocity or 4 ticks pass
type = VelSet
trigger1 = abs(vel x) < 2
trigger2 = Time = 4
x = 0

[State 0, DEATH!] ;Are you dead?
type = ChangeState
trigger1 = !alive
value = 5050

[State 0, VoiceP2LowHealth1]
type = PlaySnd
triggerall = Var(8) = 1 && alive
trigger1 = time = 0
value = 4,3
channel = 0
volume = 255

[State 0, VoiceP2LowHealth2]
type = PlaySnd
triggerall = Var(8) = 2 && alive
trigger1 = time = 0
value = 4,2
channel = 0
volume = 255

[State 0, VoiceP2LowHealth2]
type = PlaySnd
triggerall = Var(8) = 3 && alive
trigger1 = time = 0
value = 4,1
channel = 0
volume = 255

[State -3, VoiceP2LowHealth2]
type = PlaySnd
triggerall = Var(8) = 4 && alive
trigger1 = time = 0
value = 4,0
channel = 0
volume = 255

[State 0, P2LowHealthVariable]
type = VarSet
trigger1 = (Var(8) = 1 || Var(8) = 2 || Var(8) = 3 || Var(8) = 4) && alive
v = 8
value = 5
