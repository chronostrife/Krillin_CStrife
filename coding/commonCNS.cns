; Walk
[Statedef 20]
type    = S
physics = S
sprpriority = 0
ctrl = 1

[State 20, 1]
type = VelSet
trigger1 = command = "holdfwd"
x = const(velocity.walk.fwd.x)

[State 20, 2]
type = VelSet
trigger1 = command = "holdback"
x = const(velocity.walk.back.x)

[State 20, 3]
type = ChangeAnim
triggerall = vel x > 0
trigger1 = Anim != 20+var(0) && Anim != 5+var(0)
trigger2 = anim = 5+var(0) && AnimTime = 0+var(0)
value = 20+var(0)

[State 20, 4]
type = ChangeAnim
triggerall = vel x < 0
trigger1 = Anim != 21+var(0) && Anim != 5+var(0)
trigger2 = anim = 5+var(0) && AnimTime = 0+var(0)
value = 21+var(0)

